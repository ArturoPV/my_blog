<h1>USER LIST</h1>

<form action="modify_my_users" method="post">
  <input type="submit" name="" value="Submit">
  <table style="width:100%">
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Age</th>
      <th>Created at</th>
      <th>Updated at</th>
    </tr>
    <% User.all.each do |user| %>
      <tr>
        <td> <input type="text" name="[users][<%= user.id %>][fname]" value="<%= user.fname %>"> </td>
        <td> <input type="text" name="[users][<%= user.id %>][lname]" value="<%= user.lname %>"></td>
        <td> <input type="text" name="[users][<%= user.id %>][age]" value="<%= user.age %>"></td>
        <td><%= user.created_at %></td>
        <td><%= user.updated_at %></td>
      </tr>
    <% end %>
  </table>

</form>
